        <activity android:name=".view.YuyueActivity" />
        <activity android:name=".view.GuanzhuActivity" />
        <activity android:name=".view.MovieActivity" />
        <activity android:name=".view.HotShowingActivity" />
        <activity android:name=".view.ShowActivity" />
